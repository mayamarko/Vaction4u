         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="128.0" minHeight="10.0" prefHeight="31.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="235.0" minHeight="10.0" prefHeight="235.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TextField fx:id="lbl_username" />
            <TextArea fx:id="lbl_result" prefHeight="199.0" prefWidth="373.0" GridPane.rowIndex="2" />
            <Button fx:id="btn_search" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#searchUser" prefHeight="25.0" prefWidth="124.0" text="Search" translateX="90.0" GridPane.rowIndex="1" />
